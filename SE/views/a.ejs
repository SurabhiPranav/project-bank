    <table class="table table-hover myTable" id="myTable">
        <thead>
          <tr>
            <th>Project Name</th>
            <th>Discription</th>
            <th>Download</th>
            <th>Delete</th>
          </tr>
        </thead>
      <tbody>
      <% for(var i=0;i<oproject.length;i++){%>
          <tr >
             <td> <form id="show<%=oproject[i].pid %>" action="/ashow" method="post">
              <input type="radio" name="proid" value="<%= oproject[i].id +"/" +oproject[i].pid %>" style="display: none;" checked>
            </form>
               <a href="#" onclick="document.getElementById('show<%=oproject[i].pid %>').submit()"> <em> <%= oproject[i].projectname %> </em></a> 
            </td>
           
          <td>
          <p><em>
            <%=oproject[i].dis %>
          </em>
        </td>
          <td>
          </p>
            <form action="/download" method="post">
              <input type="radio" name="dlink" value="<%= oproject[i].id +"/" +oproject[i].pid %>" style="display: none;" checked>
              <input type="radio" name="pname" value="<%= oproject[i].projectname %>" style="display: none;" checked>
              <button type="submit" class="btn btn-primary btn-sm">download</button>

            </form>
          </td>
          <td>
            <form action="/adelete" id="delete<%=oproject[i].pid%>" method="post">
              <input type="radio" name="proid" value="<%= oproject[i].pid %>" style="display: none;" checked>
              <input type="radio" name="name" value="<%= oproject[i].id %>" style="display: none;" checked>
