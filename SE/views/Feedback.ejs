      </li><!-- End Login Page Nav -->

    </ul>

  </aside> <!-- End Sidebar-->

  <main id="main" class="main">

    <h5 class="modal-title" id="exampleModalLabel">Feedback Form</h5>
    
  <div class="modal-body">
    <form action="/feedback" method="post" id="feedback1">
      <div class="form-group">
        <label for="exampleFormControlTextarea1">How likely you would like to recommand us to your friends?</label>
        <div class="rating-input-wrapper d-flex justify-content-between mt-2">
          <label><input type="radio" value="1" name="rating" /><span class="border rounded px-3 py-2">1</span></label>
          <label><input type="radio" value="2" name="rating" /><span class="border rounded px-3 py-2">2</span></label>
          <label><input type="radio" value="3"name="rating" /><span class="border rounded px-3 py-2">3</span></label>
          <label><input type="radio" value="4"name="rating" /><span class="border rounded px-3 py-2">4</span></label>
          <label><input type="radio" value="5"name="rating" /><span class="border rounded px-3 py-2">5</span></label>
          <label><input type="radio" value="6"name="rating" /><span class="border rounded px-3 py-2">6</span></label>
          <label><input type="radio" value="7"name="rating" /><span class="border rounded px-3 py-2">7</span></label>
          <label><input type="radio" value="8"name="rating" /><span class="border rounded px-3 py-2">8</span></label>
          <label><input type="radio"value="9" name="rating" /><span class="border rounded px-3 py-2">9</span></label>
          <label><input type="radio" value="10"name="rating" /><span class="border rounded px-3 py-2">10</span></label>
        </div>
        <div class="rating-labels d-flex justify-content-between mt-1">
          <label>Very unlikely</label>
          <label>Very likely</label>
        </div>
      </div>
      <input type="radio" name="uid" value='<%= u[0].id %>' style="display: none;" checked>
      <div class="form-group">
        <label for="input-two">Would you like to say something?</label>
        <input type="text" class="form-control"  name="message">
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="submit"  onclick="document.getElementById('feedback1').submit();" class="btn btn-primary" data-dismiss="modal">Submit</button>
      </div>

  </main><!-- End #main -->

